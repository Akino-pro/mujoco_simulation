    <mesh name="cardboard_mesh" file="objects/Cardboard box/Cardboard box.obj" scale="0.15 0.15 0.15"/>

    <!-- Materials -->
    <material name="floorplane" texture="texplane" texuniform="true" texrepeat="2 2"
              specular="0" shininess="0" reflectance="0.01"/>
    <material name="table_wood" texture="tex-wood" texuniform="true" texrepeat="1 1"
              specular="0.35" shininess="0.4" reflectance="0.15"/>
