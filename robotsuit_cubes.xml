<?xml version="1.0" encoding="utf-8"?>
<mujoco model="gen3_scene">
  <compiler angle="radian" autolimits="true"/>

  <default>
    <!-- Contact defaults (stable-ish) -->
    <geom friction="1.0 0.01 0.0001"
          solref="0.02 1"
          solimp="0.9 0.95 0.001"
          margin="0.001"
          condim="4"/>
    <!-- Give hinge joints some damping (arm won't jiggle) -->
    <joint damping="1.0" armature="0.01"/>
  </default>

  <option timestep="0.002"
          gravity="0 0 -9.81"
          integrator="RK4"
          impratio="20"
          density="1.2"
          viscosity="2e-05"
          cone="elliptic"/>

  <size njmax="5000" nconmax="5000"/>

  <visual>
    <global offwidth="1920" offheight="1080" fovy="59"/>
    <map znear="0.001"/>
  </visual>

  <asset>
    <!-- ================= Robosuite arena assets ================= -->
    <texture type="skybox" colorspace="auto" builtin="gradient"
             rgb1="0.9 0.9 1" rgb2="0.2 0.3 0.4" width="256" height="1536"/>
    <texture type="2d" colorspace="sRGB" name="texplane"
             file="textures/light-gray-floor-tile.png"/>
    <texture type="2d" colorspace="sRGB" name="tex-wood"
             file="textures/light_wood.png"/>
    <texture type="cube" colorspace="linear" name="tex-steel-brushed"
             file="textures/steel-brushed.png"/>
    <texture type="2d" colorspace="linear" name="tex-light-gray-plaster"
             file="textures/light-gray-plaster.png"/>
    <texture name="teddy_tex_png" type="2d"
             file="objects/teddy_bear/10607_teddy_bear_SG_v1_diffuse.png"/>
    <texture name="cardboard_tex" type="2d"
             file="objects/Cardboard box/6857659-light-brown-wallpaper.png"/>

    <!-- ================= Robot meshes/materials (INLINED) ================= -->
    <mesh name="base_link" content_type="model/stl" file="robot/base_link.stl"/>
    <mesh name="shoulder_link" content_type="model/stl" file="robot/shoulder_link.stl"/>
    <mesh name="half_arm_1_link" content_type="model/stl" file="robot/half_arm_1_link.stl"/>
    <mesh name="half_arm_2_link" content_type="model/stl" file="robot/half_arm_2_link.stl"/>
    <mesh name="forearm_link" content_type="model/stl" file="robot/forearm_link.stl"/>
    <mesh name="spherical_wrist_1_link" content_type="model/stl" file="robot/spherical_wrist_1_link.stl"/>
    <mesh name="spherical_wrist_2_link" content_type="model/stl" file="robot/spherical_wrist_2_link.stl"/>
    <mesh name="bracelet_no_vision_link" content_type="model/stl" file="robot/bracelet_no_vision_link.stl"/>
    <mesh name="robotiq_base" content_type="model/stl" file="robot/robotiq_base.stl"/>
    <mesh name="left_knuckle" content_type="model/stl" file="robot/left_knuckle.stl"/>
    <mesh name="right_knuckle" content_type="model/stl" file="robot/right_knuckle.stl"/>
    <mesh name="left_finger" content_type="model/stl" file="robot/left_finger.stl"/>
    <mesh name="right_finger" content_type="model/stl" file="robot/right_finger.stl"/>
    <mesh name="left_inner_knuckle" content_type="model/stl" file="robot/left_inner_knuckle.stl"/>
    <mesh name="right_inner_knuckle" content_type="model/stl" file="robot/right_inner_knuckle.stl"/>
    <mesh name="left_finger_tip" content_type="model/stl" file="robot/left_finger_tip.stl"/>
    <mesh name="right_finger_tip" content_type="model/stl" file="robot/right_finger_tip.stl"/>
    <mesh name="bracelet_with_vision_link" content_type="model/stl" file="robot/bracelet_with_vision_link.stl"/>

    <mesh name="teddy_mesh" file="objects/teddy_bear/teddy3.obj" scale="0.01 0.01 0.01"/>
