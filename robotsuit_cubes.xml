    <material name="walls_mat" texture="tex-light-gray-plaster" texuniform="true" texrepeat="3 3"
              specular="0.1" shininess="0.1"/>
    <material name="teddy_mat_png" texture="teddy_tex_png" specular="0.05" shininess="0.2"/>
    <material name="cardboard_mat" texture="cardboard_tex" texuniform="true" specular="0.05" shininess="0.2"/>
  </asset>

