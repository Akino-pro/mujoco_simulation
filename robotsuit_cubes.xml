            fromto="0.05 0 1.0668   -0.05 0 1.0668"
            size="0.010"
            rgba="0.2 0.2 0.2 1"
            contype="0" conaffinity="0" group="1"/>

      <!-- Small camera box at the arm end, aligned with fixed_down camera position -->
