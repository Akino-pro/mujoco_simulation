      <geom name="fixed_cam_baseplate" type="cylinder"
            size="0.06 0.01"
            pos="0.05 0 0"
            rgba="0.15 0.15 0.15 1"
            contype="0" conaffinity="0" group="1"/>

      <!-- Vertical pole: from table top (z=0.72) up to camera height (z=1.7868) -->
