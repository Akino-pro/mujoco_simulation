      <geom name="fixed_cam_pole" type="capsule"
            fromto="0.05 0 0   0.05 0 1.0668"
            size="0.012"
            rgba="0.2 0.2 0.2 1"
            contype="0" conaffinity="0" group="1"/>

      <!-- Horizontal arm at the top: 5cm toward robot table (negative X) -->
