            quat="0.9659 0 0.2588 0"
            size="0.01 0.02"
            rgba="0.12 0.12 0.12 1"
            contype="0" conaffinity="0" group="1"/>
    </body>

    <!-- Light -->
    <light pos="1 1 1.5" dir="-0.19245 -0.19245 -0.96225" type="directional" castshadow="false"/>

